<div class="col-sm-12 elective-type" id="CSE">
	<h2><span class="toggle-elective" value="CSE-container" onclick="toggleElectiveContainer(this)">+</span>Complementary Studies Electives</h2>
	<div class="col-sm-12" id="CSE-container" style="display:none;">
		<h5>Two courses from List C and two courses from List A, C, or D</h5>
		<div class="col-sm-12">
			<h4>List A - Impact Courses</h4>
			<div class="list-of-electives col-sm-12">
				<% @cse_electives.each do |elective| %>
					<% if elective["list"] == "A" %>
						<div class="elective-cell col-sm-1" data-toggle="modal" data-target="#course-detail-modal" title="<%=elective["title"]%>" value="<%=elective["code"]%>" onclick="loadDetailsPopup(this, false)">
				            <%= elective["code"] %>
			            </div>
			        <% end %>
				<% end %>
			</div>
		</div>
		<div class="col-sm-12">
			<h4>List C - Humanities and Social Sciences Courses</h4>
			<div class="elective-sub-category col-sm-12">
				<h5>1. Pre-scheduled - normally at 11:30 MWF, 7-10 M, or 7-10 T and therefore conflict-free for most Engierring students</h5>
				<div class="list-of-electives col-sm-12">
					<% @cse_electives.each do |elective| %>
						<% if elective["list"] == "C-1" %>
							<div class="elective-cell col-sm-1" data-toggle="modal" data-target="#course-detail-modal" title="<%=elective["title"]%>" value="<%=elective["code"]%>" onclick="loadDetailsPopup(this, false)">
					            <%= elective["code"] %>
				            </div>
				        <% end %>
					<% end %>
				</div>
			</div>
			<div class="elective-sub-category col-sm-12">
				<h5>2. Non Pre-scheduled</h5>
				<div class="list-of-electives col-sm-12">
					<% @cse_electives.each do |elective| %>
						<% if elective["list"] == "C-2" %>
							<div class="elective-cell col-sm-1" data-toggle="modal" data-target="#course-detail-modal" title="<%=elective["title"]%>" value="<%=elective["code"]%>" onclick="loadDetailsPopup(this, false)">
					            <%= elective["code"] %>
				            </div>
				        <% end %>
					<% end %>
				</div>		
			</div>
		</div>
		<div class="col-sm-12">
			<h4>List D - Other Permissible Complementary Studies Courses</h4>
			<div class="list-of-electives col-sm-12">
				<% @cse_electives.each do |elective| %>
					<% if elective["list"] == "D" %>
						<div class="elective-cell col-sm-1" data-toggle="modal" data-target="#course-detail-modal" title="<%=elective["title"]%>" value="<%=elective["code"]%>" onclick="loadDetailsPopup(this, false)">
				            <%= elective["code"] %>
			            </div>
			        <% end %>
				<% end %>
			</div>
		</div>
	</div>
</div>
<br />
<div class="col-sm-12 elective-type" id="NSE">
	<h2><span class="toggle-elective" value="NSE-container" onclick="toggleElectiveContainer(this)">+</span>Natural Science Electives</h2>
	<div class="col-sm-12" id="NSE-container" style="display:none;">
		<h5>Two courses : at least one from List 1 and at most one from List 2</h5>
